{
    "defaultSeverity": "error",
    "extends": [
        "tslint-microsoft-contrib"
    ],
    "jsRules": {},
    "rules": {
        "no-backbone-get-set-outside-model": [
            false
        ],
        "typedef": [
            true,
            "call-signature",
            "arrow-call-signature",
            "parameter",
            "arrow-parameter",
            "property-declaration",
            "member-variable-declaration",
            "object-destructuring",
            "array-destructuring"
        ],
        "trailing-comma": [
            true,
            {
                "multiline": "always",
                "singleline": "never"
            }
        ]
    },
    "rulesDirectory": []
}